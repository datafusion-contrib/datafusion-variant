statement ok
CREATE TABLE json_data (id INT, json_str TEXT) AS VALUES
  (7, '{"name": "Alice", "age": 30}'),
  (6, '{"name": "Bob", "age": 25}'),
  (5, '{"items": [1, 2, 3], "count": 3}'),
  (4, 'null'),
  (3, '"simple string"'),
  (2, '123'),
  (1, 'true');

# convert json strings to variant and back to json string (round trip)
query IT
SELECT id, variant_pretty(json_to_variant(json_str)) FROM json_data ORDER BY id;
----
1 BooleanTrue
2 Int8(123)
3 ShortString(ShortString("simple string"))
4 Null
5 {"count": Int8(3), "items": [Int8(1), Int8(2), Int8(3)]}
6 {"age": Int8(25), "name": ShortString(ShortString("Bob"))}
7 {"age": Int8(30), "name": ShortString(ShortString("Alice"))}